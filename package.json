    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^24.5.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.9",
    "nyc": "^13.3.0",
    "smee-client": "^1.0.2",
    "standard": "^10.0.3",
    "ts-jest": "^24.0.1",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^18.0.0"
